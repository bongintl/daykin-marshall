{
  "require": {
    "aelvan/imager": "^2.1",
    "craftcms/cms": "^3.0.0",
    "craftcms/redactor": "^2.3",
    "ether/seo": "^3.5",
    "fortrabbit/craft-copy": "^1.0.0-RC5",
    "pinfirestudios/craft-twig-lambda": "@dev",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
