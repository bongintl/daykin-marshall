        <ul>
            {% for text, url in {
                "People": "/people",
                "Making": "/making",
                "Buildings": "/buildings",
            } %}
                <li><a href="{{ url }}"><dms-engraved>{{ text }}</dms-engraved></a></li>
            {% endfor %}
        </ul>
    </div>
    
    <div class="nav__group nav__group--tr">
        <ul>
            <li><a href="/"><dms-engraved>Daykin Marshall Studio</dms-engraved></a></li>
        </ul>
