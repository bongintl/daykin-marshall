    {% if nav.showAddress %}
        
        <div class="nav__group nav__group nav__group--bl nav__group--bl--address">
            <ul>
                {% for link in nav.links.all %}
                    <li><a href="{{ link.linkUrl }}" {% if link.newTab %} target="_blank"{% endif %}><dms-engraved>{{ link.linkTitle }}</dms-engraved></a></li>
                {% endfor %}
                <li><a href="tel:{{ contact.phoneNumber }}">
                    <span class="s-show m-hide"><dms-engraved>Phone</dms-engraved></span>
                    <span class="s-hide m-show"><dms-engraved>{{ contact.phoneNumber }}</dms-engraved></span>
                </a></li>
                <li><a href="mailto:{{ contact.email }}">
                    <span class="s-show m-hide"><dms-engraved>Email</dms-engraved></span>
                    <span class="s-hide m-show"><dms-engraved>{{ contact.email }}</dms-engraved></span>
                </a></li>
